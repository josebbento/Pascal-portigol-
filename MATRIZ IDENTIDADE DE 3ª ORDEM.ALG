algoritmo "matriz identidade de n ordem"
var
   x,y,fim:inteiro
inicio
      fim<-4
      para x <- 1 ate fim faça
           para y <- 1 ate fim faça
                se (x = y) então
                   escreva(1:3)
                senão
                     escreva(0:3)
                fimse
           fimpara
           escreval()
      fimpara
fimalgoritmo